<?xml version="1.0" encoding="utf-8"?>
<OpenRoadsSightVisibility xmlns="OpenRoads">
    <Methods>
        <!--                                                                                         -->
        <!-- Valid methodTypes: Table, Italian_SSD, AASHTO_2011_SSD, Australian_SSD, French_ARP_SSD  -->
        <!--                                                                                         -->
		
        <!--                                Italian Standard Variables and Constant (Method = Italian_SSD)                               -->
        <!-- Calculations based on Norme Funzionali e geometriche per la costruzione delle strade – Decreto Ministeriale 5 Novembre 2001 -->
        <!-- Valid Italian roadCategory: ExtraUrban, Urban                                                                               -->
        <!-- double ro = 1.15; // air mass (default value = 1.15)                                                                        -->
        <!-- double cx = 0.35; // aerodynamic coefficient (default value = 0.35)                                                         -->
        <!-- double S = 2.1;   // surface resistance (default value = 2.1)                                                               -->
        <!-- double m = 1250;  // vehicle mass (default value = 1250)                                                                    -->
        <!-- double Res = 0.0; // Rolling resistance                                                                                     -->
        <!-- double t = 0.0;                                                                                                             -->
        <!-- double V_interval = 0.1;                                                                                                    -->
        <!-- double gravityConst = 9.81;                                                                                                 -->
        <!-- int roundingMultiple // Computed Required Sight distances will be rounded to this multiple.  0 indicates no rounding.-->
        <SightDistanceMethod name ="Italian SSD Extra Urban Via Features" methodType ="Italian_SSD">
            <EyePosition interval="15.0" offset="0.0" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables  roadCategory ="ExtraUrban" ro ="1.15" cx="0.35" S="2.1" m="1250" Res="0.0" t="0.0" V_interval="0.1" gravityConstant="9.81" roundingMultiple="0"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="Italian SSD Extra Urban Via CL Offsets" methodType ="Italian_SSD">
            <EyePosition interval="15.0" offset="0.65" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="3.0" height="0.60"/>
            <StandardVariables roadCategory ="ExtraUrban" ro ="1.15" cx="0.35" S="2.1" m="1250" Res="0.0" t="0.0" V_interval="0.1" gravityConstant="9.81" roundingMultiple="0"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="Italian SSD Urban Via Features" methodType ="Italian_SSD">
            <EyePosition interval="15.0" offset="0.0" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables roadCategory ="Urban" ro ="1.15" cx="0.35" S="2.1" m="1250" Res="0.0" t="0.0" V_interval="0.1" gravityConstant="9.81" roundingMultiple="0"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="Italian SSD Urban Via CL Offsets" methodType ="Italian_SSD">
            <EyePosition interval="15.0" offset="0.65" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="3.0" height="0.60"/>
            <StandardVariables roadCategory ="Urban" ro ="1.15" cx="0.35" S="2.1" m="1250" Res="0.0" t="0.0" V_interval="0.1" gravityConstant="9.81" roundingMultiple="0"/>
        </SightDistanceMethod>
        <!--                   2011 adn 2018 AASHTO Standard Variables and Constants (Method = AASHTO_2011_SSD)                   -->
		<!-- Calculations based on 2011 AASHTO A Policy on Geometric Design of Highways and Streets, Chapter 3                    -->
        <!-- double reactionTime  //Brake reaction time. Defaults to 2.5 seconds if not defined in this settings file             -->
        <!-- double gradeThreshold // Grades(m/m) less than gradeThreshold will use SSD equation 3-2                              -->
        <!--                       // Grades(m/m) greater than or equal to the gradeThreshold will use SSD equation 3-3           -->
        <!-- double gravityConst = 9.81; // meters/sec^2                                                                          -->
        <!-- int roundingMultiple // Computed Required Sight distances will be rounded to this multiple.  0 indicates no rounding.-->
        <SightDistanceMethod name ="2011 AASHTO Stopping Site Distance - Zero Offset" methodType ="AASHTO_2011_SSD" >
            <EyePosition interval="15.0" offset="0.0" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables reactionTime="2.5" gravityConstant="9.81" gradeThreshold ="0.03" roundingMultiple="1"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="2011 AASHTO Stopping Site Distance - Eye 3 meter Offset" methodType ="AASHTO_2011_SSD" >
            <EyePosition interval="15.0" offset="3" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables reactionTime="2.5" gravityConstant="9.81" gradeThreshold ="0.03" roundingMultiple="1"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="2018 AASHTO Stopping Site Distance - Zero Offset" methodType ="AASHTO_2011_SSD" >
            <EyePosition interval="15.0" offset="0.0" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables reactionTime="2.5" gravityConstant="9.81" gradeThreshold ="0.03" roundingMultiple="1"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="2018 AASHTO Stopping Site Distance - Eye 3 meter Offset" methodType ="AASHTO_2011_SSD" >
            <EyePosition interval="15.0" offset="3" height="1.08"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.60"/>
            <StandardVariables reactionTime="2.5" gravityConstant="9.81" gradeThreshold ="0.03" roundingMultiple="1"/>
        </SightDistanceMethod>
        
        <!--                         2016 Australian Standard Variables and Constants (Method = Australian_SSD)                        -->
        <!-- Calculations based on 2016 Austroads: Guide to Road Design Part 3: Geometric Design, Section 5.3: Stopping Sight Distance -->
        <!-- double reactionTime  //Brake reaction time. Defaults to 2.5 seconds if not defined in this settings file                  -->
        <!-- double coefficientOfDeceleration // Defaults to 0.36 if not defined in this settings file                                 -->
        <!-- double maxCoefficientOfDeceleration // Defaults to 0.46 if not defined. Used to compute Relaxed SSD                       -->
        <!-- int roundingMultiple // Computed Required Sight distances will be rounded to this multiple.  0 indicates no rounding.     -->
        <SightDistanceMethod name ="2016 Australian SSD Via Features" methodType ="Australian_SSD" >
            <EyePosition interval="15.0" offset="0.0" height="1.10"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="0.0" height="0.20"/>
            <StandardVariables coefficientOfDeceleration ="0.36" maxCoefficientOfDeceleration ="0.46" reactionTime="2.0" roundingMultiple="0"/>
        </SightDistanceMethod>
        <SightDistanceMethod name ="2016 Australian SSD Via CL Offsets" methodType ="Australian_SSD" >
            <EyePosition interval="15.0" offset="0.65" height="1.10"/>
            <ObjectPosition moveTargetToAchieveVisibility="false" interval="5.0" offset="3.0" height="0.20"/>
            <StandardVariables coefficientOfDeceleration ="0.36" maxCoefficientOfDeceleration ="0.46" reactionTime="2.0" roundingMultiple="0"/>
        </SightDistanceMethod>
        
        <!--                         French ARP Standard Variables and Constants (Method = French_ARP_SSD)                         -->
		<!-- Calculations based on Aménagement des Routes Principales (ARP) - Guide technique, Chapitre 4 : Visibilité             -->
